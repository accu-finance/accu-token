{
  "name": "@accu-finance/accu-token",
  "version": "1.0.0",
  "description": "Accu ERC20 token",
  "files": [
    "/contracts/**/*.sol"
  ],
  "scripts": {
    "prepare": "node ./.setup.js && npm run compile",
    "lint": "eslint \"**/*.{js,ts}\" && solhint contracts/**/*.sol",
    "lint:fix": "eslint --fix \"**/*.{js,ts}\" && solhint --fix contracts/**/*.sol",
    "format": "npx prettier --check \"**/*.{ts,js,sol}\"",
    "format:fix": "npx prettier --write \"**/*.{ts,js,sol}\"",
    "compile": "cross-env SKIP_LOAD=true hardhat compile",
    "test": "cross-env TS_NODE_TRANSPILE_ONLY=1 hardhat test",
    "coverage": "npx hardhat coverage --network hardhat",
    "clean": "rm -rf ./artifacts ./cache ./typechain"
  },
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "npm:hardhat-deploy-ethers@^0.3.0-beta.10",
    "@nomiclabs/hardhat-solhint": "^2.0.0",
    "@nomiclabs/hardhat-waffle": "^2.0.1",
    "@openzeppelin/contracts": "3.0.1",
    "@typechain/ethers-v5": "^6.0.1",
    "@types/chai": "^4.2.15",
    "@types/lowdb": "^1.0.9",
    "@types/mocha": "^8.2.1",
    "@types/node": "^14.14.31",
    "@typescript-eslint/eslint-plugin": "^4.15.2",
    "@typescript-eslint/parser": "^4.15.2",
    "chai": "^4.3.0",
    "chai-ethers": "0.0.1",
    "cross-env": "^7.0.3",
    "dotenv": "^8.2.0",
    "eslint": "^7.21.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eth-sig-util": "^3.0.1",
    "ethereum-waffle": "^3.3.0",
    "ethereumjs-util": "^7.0.2",
    "ethers": "^5.4.6",
    "hardhat": "^2.3.0",
    "hardhat-deploy": "^0.9.0",
    "hardhat-deploy-ethers": "^0.3.0-beta.10",
    "hardhat-gas-reporter": "^1.0.4",
    "hardhat-typechain": "^0.3.5",
    "lowdb": "1.0.0",
    "prettier": "^2.3.2",
    "prettier-plugin-solidity": "^1.0.0-beta.17",
    "solhint": "^3.3.2",
    "solhint-plugin-prettier": "0.0.5",
    "solidity-coverage": "^0.7.15",
    "ts-generator": "^0.1.1",
    "ts-node": "^9.1.1",
    "tslint-config-prettier": "^1.18.0",
    "typechain": "^4.0.2",
    "typescript": "^4.2.2"
  },
